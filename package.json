  },
  "devDependencies": {
    "@types/history": "^4.7.6",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "history": "^4.10.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "total-functions": "^1.0.0",
    "ts-jest": "^26.0.0",
    "type-coverage": "^2.6.1",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "oaf-routing": "^4.0.0"
  },
  "peerDependencies": {
    "history": "^4.10.1"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx --rulesdir node_modules/total-functions/dist",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx}'",
    "documentation": "typedoc --excludeNotExported --excludePrivate --out docs src && cp docs_config.yml ./docs/_config.yml",
    "release": "yarn build && yarn lint && yarn type-coverage && yarn format-lint && yarn publish && yarn documentation",
    "test": "jest"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
