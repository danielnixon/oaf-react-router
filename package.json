  "main": "dist",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oaf-project/oaf-react-router.git"
  },
  "devDependencies": {
    "@types/history": "^4.7.5",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "history": "^4.10.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "total-functions": "^0.1.0",
    "ts-jest": "^25.4.0",
    "type-coverage": "^2.4.0",
    "typedoc": "^0.16.10",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "oaf-routing": "^4.0.0"
  },
  "peerDependencies": {
    "history": "^4.10.1"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx --rulesdir node_modules/total-functions/dist",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx}'",
    "documentation": "typedoc --excludeNotExported --excludePrivate --out docs src && cp docs_config.yml ./docs/_config.yml",
    "release": "yarn build && yarn lint && yarn type-coverage && yarn format-lint && yarn publish && yarn documentation",
    "test": "jest"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
